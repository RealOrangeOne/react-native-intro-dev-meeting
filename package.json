{
  "name": "react-native-intro-dev-meeting",
  "version": "1.0.0",
  "description": "React Native Talk",
  "main": "index.js",
  "scripts": {
    "create-build-dirs": "mkdir -p build/js build/css",
    "start": "node scripts/server.js",
    "build-js": "bash scripts/build-js.sh",
    "build-less": "lessc --silent src/less/style.less build/css/style.css",
    "build-html": "cp src/index.html build/index.html",
    "build": "npm run create-build-dirs && npm run build-js && npm run build-less && npm run build-html"
  },
  "author": "Jake Howard",
  "devDependencies": {
    "babel-preset-es2015": "=6.6.0",
    "babelify": "=7.2.0",
    "browserify": "=13.0.0",
    "less": "=2.6.0"
  },
  "dependencies": {
    "connect": "=3.4.1",
    "highlight.js": "=9.2.0",
    "serve-static": "=1.10.2",
    "reveal.js": "=3.2.0"
  }
}
